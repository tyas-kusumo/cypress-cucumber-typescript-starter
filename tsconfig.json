{
  "compilerOptions": {
    "outDir": "build",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "typeRoots": ["node_modules/@types", "cypress/support"],
    "strict": true,
    "allowJs": true
  },
  "include": [
    "node_modules/cypress",
    "cypress/**/*.ts",
    "cypress/support/index.js",
    "cypress/plugins/index.js",
    "husky.config.js"
  ]
}
